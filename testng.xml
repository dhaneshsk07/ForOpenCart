<suite name="OpenCart Suite" parallel="tests" thread-count="2">

<listeners>
    <listener class-name="nnd.Listners.MyTestListener"/>
</listeners>
   
    
    <test name="OpenCart Functional Tests">
      
        <classes>
            <!-- Replace with actual package names and test classes -->
